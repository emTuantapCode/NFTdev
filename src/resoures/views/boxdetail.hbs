<div class="detail-box-container">
    <div class="detail-box-body">
        <div class="detail-box-back">
            <a href="/herobox">
                <img src="data:image/png;base64,"
                    alt="back" class="img-fluid">
                BACK
            </a>
        </div>
        <div class="row">
            <div class="col-4">
                <div class="detail-box-img">
                    <img src="{{boxsale.img}}" alt="box" class="img-fluid">
                </div>
                <div class="detail-box-price">{{boxsale.price}}</div>
            </div>
            <div class="col-8">
                <div class="detail-box-name">
                    <div style="color: var(--{{boxsale.slug}});" class="box-by-namebg">{{boxsale.name}}</div>
                    <div style="color: var(--{{boxsale.slug}});" class="box-by-name">{{boxsale.name}}</div>
                </div>
                <div class="detail-box-table">
                    <div class="row header-tb">DROP RATE DETAIL</div>
                    <div class="row">
                        <div class="col">Common</div>
                        <div class="col">Rare</div>
                        <div class="col">Epic</div>
                        <div class="col">Legend</div>
                    </div>
                    <div class="row">
                        <div class="col">{{boxsale.originrate}} thứ 0</div>
                        <div class="col">{{boxsale.originrate}} thứ 1</div>
                        <div class="col">{{boxsale.originrate}} thứ 2</div>
                        <div class="col">{{boxsale.originrate}} thứ 3</div>
                    </div>
                    <div class="row">
                        <div class="col">{{boxsale.chormasrate}}</div>
                        <div class="col">Otto</div>
                        <div class="col">Otto</div>
                        <div class="col">Otto</div>
                    </div>
                    <div class="row">
                        <div class="col">{{boxsale.prestigerate}}</div>
                        <div class="col">Otto</div>
                        <div class="col">Otto</div>
                        <div class="col">Otto</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>